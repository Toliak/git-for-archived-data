{
    "$schema": "https://json.schemastore.org/prettierrc",
    "arrowParens": "avoid",
    "singleQuote": true,
    "trailingComma": "all",
    "tabWidth": 4,
    "printWidth": 80,
    "semi": true,

    "plugins": ["prettier-plugin-xml-msword"],
    "xmlWhitespaceSensitivity": "strict",
    "xmlSelfClosingSpace": false,
    "xmlExpandSelfClosingTags": true
}
